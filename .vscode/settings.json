{
    "direnv.restart.automatic": true,
    "typescript.tsdk": "node_modules/typescript/lib",
    "typescript.enablePromptUseWorkspaceTsdk": true,
    "editor.defaultFormatter": "EditorConfig.EditorConfig",
    "[typescript][typescriptreact][javascript][javascriptreact][json][jsonc]": {
        "editor.defaultFormatter": "esbenp.prettier-vscode"
    },

    "files.exclude": {
        "**/.git": true,
        "**/.svn": true,
        "**/.hg": true,
        "**/.DS_Store": true,
        "**/Thumbs.db": true,
        "**/.direnv": true,
        "**/.tsbuildinfo": true,
        "**/build": true,
        "**/coverage": true,
        "**/dist": true
    },
    "search.exclude": {
        "**/node_modules": true,
        "**/bower_components": true,
        "**/*.code-search": true,
        "submodules/nvlist/**": true,
        "submodules/osxcross/**": true,
        "submodules/wintun/**": true,
        "submodules/wireguard-apple/**": true,
        "submodules/wireguard-go/**": true,
        "submodules/wireguard-tools/**": true,
        "submodules/wireguard-windows/**": true
    },

    "cSpell.enabled": true,
