        "patchedDependencies": {
            "@rushstack/heft@0.66.0": "patches/@rushstack__heft@0.66.0.patch",
            "@rushstack/heft-jest-plugin@0.11.21": "patches/@rushstack__heft-jest-plugin@0.11.21.patch",
