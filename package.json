{
    "name": "the-wireguard-effect",
    "version": "0.0.1-alpha.29",
    "description": "Cross platform wireguard api client for nodejs built on wireguard-go with effect-ts",
    "keywords": [
        "wireguard",
        "vpn",
        "effect-ts"
    ],
    "homepage": "https://github.com/leonitousconforti/the-wireguard-effect",
    "bugs": {
        "url": "https://github.com/leonitousconforti/the-wireguard-effect/issues",
        "email": "leo@leoconforti.us"
    },
    "repository": {
        "type": "git",
        "url": "git+https://github.com/leonitousconforti/the-wireguard-effect.git"
    },
    "license": "GPL-3.0-only",
    "author": "Leo Conforti <leo@leoconforti.us> (https://leoconforti.us)",
    "type": "module",
    "main": "./dist/cjs/index.js",
    "module": "./dist/esm/index.js",
    "types": "./dist/dts/index.d.ts",
    "directories": {
        "doc": "docs",
        "example": "examples",
        "test": "test"
    },
    "scripts": {
        "build": "pnpm clean && pnpm codegen && pnpm build-esm && pnpm build-cjs && pnpm build-annotate && build-utils pack-v2 && pnpm build-submodules && docgen",
        "build-annotate": "babel build --plugins annotate-pure-calls --out-dir build --source-maps",
        "build-cjs": "babel build/esm --plugins @babel/transform-export-namespace-from --plugins @babel/transform-modules-commonjs --out-dir build/cjs --source-maps",
        "build-esm": "tsc -b tsconfig.build.json",
        "build-submodules": "./submodules/build.mjs",
        "check": "tsc -b tsconfig.json",
        "clean": "rimraf .tsbuildinfo docs build dist coverage && rimraf --glob src/internal/*-wireguard-go src/internal/*-wg-quick src/internal/*-wintun.dll src/internal/LICENSE-* src/internal/*.exe",
        "codegen": "build-utils prepare-v2",
        "coverage": "vitest --coverage",
        "lint": "eslint .",
        "test": "vitest"
    },
    "resolutions": {
        "rollup": "npm:@rollup/wasm-node"
    },
    "dependencies": {
        "@vscode/sudo-prompt": "^9.3.1",
        "execa": "^8.0.1",
        "ini": "^4.1.2"
    },
    "devDependencies": {
        "@babel/cli": "7.24.1",
        "@babel/core": "7.24.4",
        "@babel/plugin-transform-export-namespace-from": "7.24.1",
        "@babel/plugin-transform-modules-commonjs": "7.24.1",
        "@effect/build-utils": "0.7.6",
        "@effect/docgen": "0.4.2",
        "@effect/eslint-plugin": "0.1.2",
        "@effect/language-service": "0.1.0",
        "@effect/platform": "0.48.27",
        "@effect/platform-node": "0.45.29",
        "@effect/printer": "0.32.3",
        "@effect/printer-ansi": "0.32.27",
        "@effect/schema": "0.64.20",
        "@effect/typeclass": "0.23.18",
        "@effect/vitest": "0.2.6",
        "@rollup/wasm-node": "4.14.1",
        "@types/ini": "4.1.0",
        "@types/node": "20.12.7",
        "@typescript-eslint/eslint-plugin": "7.6.0",
        "@typescript-eslint/parser": "7.6.0",
        "@typescript-eslint/utils": "7.6.0",
        "@vitest/coverage-v8": "1.4.0",
        "babel-plugin-annotate-pure-calls": "0.4.0",
        "effect": "2.4.18",
        "eslint": "8.57.0",
        "eslint-config-prettier": "9.1.0",
        "eslint-import-resolver-typescript": "3.6.1",
        "eslint-plugin-codegen": "0.28.0",
        "eslint-plugin-deprecation": "2.0.0",
        "eslint-plugin-import": "2.29.1",
        "eslint-plugin-prettier": "5.1.3",
        "eslint-plugin-sort-destructure-keys": "1.5.0",
        "fast-check": "3.17.1",
        "list-item": "2.0.0",
        "markdown-link": "0.1.1",
        "mixin-deep": "1.3.2",
        "object.pick": "1.3.0",
        "prettier": "3.2.5",
        "prettier-plugin-jsdoc": "1.3.0",
        "prettier-plugin-packagejson": "2.4.14",
        "remarkable": "1.7.4",
        "rimraf": "5.0.5",
        "tsx": "4.7.2",
        "typescript": "5.4.5",
        "vite": "5.2.8",
        "vitest": "1.4.0",
        "ws": "8.16.0"
    },
    "peerDependencies": {
        "@effect/platform": "0.48.27",
        "@effect/platform-node": "0.45.29",
        "@effect/schema": "0.64.20",
        "effect": "2.4.18"
    },
    "packageManager": "pnpm@8.15.4",
    "engines": {
        "node": ">=18.0.0"
    },
    "publishConfig": {
        "access": "public",
        "directory": "dist",
        "executableFiles": [
            "prebuilds/linux-wg-quick",
            "prebuilds/darwin-wg-quick",
            "prebuilds/freebsd-wg-quick",
            "prebuilds/openbsd-wg-quick"
        ],
        "provenance": true
    },
    "pnpm": {
        "patchedDependencies": {
            "@effect/build-utils@0.7.6": "patches/@effect__build-utils@0.7.6.patch",
            "@effect/docgen@0.4.2": "patches/@effect__docgen@0.4.2.patch"
        }
    }
}
