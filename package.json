        "@effect/schema": "0.64.0",
        "@effect/typeclass": "0.23.4",
        "@microsoft/api-extractor": "7.42.3",
        "@protobuf-ts/runtime": "2.9.4",
        "@protobuf-ts/runtime-rpc": "2.9.4",
